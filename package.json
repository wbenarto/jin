  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "scss": "node-sass --watch styles/scss -o styles/css",
    "test": "node_modules\\.bin\\cypress run",
    "headTest": "npm run test -- --headed",
