    "node-sass": "^6.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "^8.32.2",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
